{
  "name": "monkey-ui",
  "version": "0.1.0",
  "title": "monkey-ui",
  "description": "一套基于Vue2.0的高质量的UI框架",
  "homepage": "https://chengllnice.github.io/monkey-ui/",
  "author": "程路路",
  "keywords": [
    "monkey-ui",
    "monkeyui",
    "vue",
    "vue.js",
    "component",
    "ui"
  ],
  "main": "./dist/monkey-ui.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/chengllNice/monkey-ui"
  },
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "clean": "rimraf dist",
    "build:docs": "rimraf dist-docs && vue-cli-service build --dest dist-docs",
    "build:locale": "rimraf dist/locale && node build/build.locale.js",
    "build:umd": "npm run clean && vue-cli-service build --target lib --name monkey-ui src/index.js && node build/dist.file.deal.js",
    "build:style": "rimraf dist/styles && gulp --gulpfile build/build.styles.js",
    "build:deploy": "npm run build:umd && npm run build:style && npm run build:locale"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "deepmerge": "^4.2.2",
    "element-resize-detector": "^1.1.15",
    "popper.js": "^1.15.0",
    "v-click-outside-x": "^4.0.15",
    "vee-validate": "^3.2.1",
    "vuescroll": "^4.13.1"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-modules-umd": "^7.10.4",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/component-compiler-utils": "^3.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-runtime": "^6.26.0",
    "cheerio": "^1.0.0-rc.3",
    "chinese2pinyin": "^1.0.1",
    "del": "^5.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-save": "^0.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-uglifycss": "^1.1.0",
    "highlight.js": "^9.15.10",
    "markdown-it": "^9.1.0",
    "markdown-it-anchor": "^5.2.4",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "markdown-loader": "^5.1.0",
    "node-sass": "^4.12.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^7.1.0",
    "transliteration": "^2.1.5",
    "uglify-js": "^3.10.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^2.6.12",
    "vue-clipboard2": "^0.3.0",
    "vue-i18n": "^8.21.1",
    "vue-router": "^3.0.3",
    "vue-template-compiler": "^2.6.10"
  }
}
